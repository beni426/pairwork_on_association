 
<table>
 <tr>
    <th>ユーザー名前</th>
    <th>ユーザーemail</th>
    <th colspan="3"></th>
  </tr>
  <% @users.each do |user| %>
 <tr>
   
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= button_to "ユーザー詳細", user_path(user.id),method: :get %></td>
    <td><%= button_to 'ユーザー編集', edit_user_path(user.id),method: :get %></td>
    <td><%= button_to '削除',user_path(user.id), method: :delete ,data:{ confirm: '本当に削除していいですか？' } %></td>
   
    <% end %>
</div>
